# Setup virtualenv wrapper
sandbox_init_virtualenvwrapper(){
  source /usr/local/bin/virtualenvwrapper.sh
}
sandbox_hook virtualenvwrapper workon
sandbox_hook virtualenvwrapper mkvirtualenv
sandbox_hook virtualenvwrapper mktmpenv
sandbox_hook virtualenvwrapper lsvirtualenv
sandbox_hook virtualenvwrapper showvirtualenv
sandbox_hook virtualenvwrapper rmvirtualenv

sandbox_init_nvm(){
    # nvm
    NVM_DIR=$HOME/.nvm
    [ -s "$NVM_DIR/nvm.sh" ] && source "$NVM_DIR/nvm.sh"
    [[ -r $NVM_DIR/bash_completion ]] && source $NVM_DIR/bash_completion
}
sandbox_hook nvm nvm
